{
  "name": "smart-reno-plugin",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently -n API,WEB -c green,cyan \"npm:dev -w apps/api\" \"npm:dev -w apps/web\"",
    "build": "npm run build -w packages/shared && npm run build -w apps/api && npm run build -w apps/web",
    "typecheck": "tsc -b"
  },
  "devDependencies": {
    "concurrently": "^9.0.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@types/react": "^19.1.13",
    "clsx": "^2.1.1",
    "lucide-react": "^0.544.0",
    "react": "^19.1.1",
    "react-hot-toast": "^2.6.0"
  }
}
